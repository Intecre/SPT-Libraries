    <Method Name="TryGetUnit" Id="{608b8c09-7058-4b13-8e8e-aa3c05cb56c3}">
      <Declaration><![CDATA[METHOD TryGetUnit: BOOL
VAR_INPUT
	index : UDINT;
END_VAR
VAR_OUTPUT
	Unit : I_PMLSubUnitInfo;
END_VAR
VAR
	baseItem : I_InterfaceBase;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[baseCollection.TryGetItem(index, item => baseItem);
Unit := CastToType(baseItem);

IF Unit = 0 THEN
	LogError( 'Cannot TryGetUnit  Unit = 0' );
	RETURN;
END_IF 

TryGetUnit := Unit <> 0;]]></ST>
      </Implementation>
