    <Method Name="AddUnit" Id="{edbc1f8a-3500-4b7c-bdb7-60b253486aba}">
      <Declaration><![CDATA[METHOD AddUnit : BOOL
VAR_INPUT
	newUnit : I_PMLSubUnitInfo;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF newUnit = 0 THEN
	LogError( 'Cannot AddUnit invalid ref' );
	RETURN;
END_IF

AddUnit := baseCollection.AddItem( newItem := CastToBase(newUnit) );]]></ST>
      </Implementation>
    </Method>
    <Method Name="CastToBase" Id="{30ce565b-3c82-4a28-94a9-23dcbd458f22}" FolderPath="Private\">
